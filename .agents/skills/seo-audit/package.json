{
  "name": "@seomator/seo-audit",
  "version": "3.0.0",
  "description": "SEOmator - Comprehensive SEO audit CLI tool with 251 rules across 20 categories",
  "type": "module",
  "bin": {
    "seomator": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "exports": {
    ".": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "electron:dev": "electron-vite dev -c electron/electron-vite.config.ts",
    "electron:build": "electron-vite build -c electron/electron-vite.config.ts",
    "electron:pack": "npm run electron:build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "seo",
    "seo-audit",
    "audit",
    "cli",
    "web-vitals",
    "core-web-vitals",
    "lighthouse",
    "meta-tags",
    "structured-data",
    "accessibility",
    "crawler",
    "site-audit",
    "javascript-rendering",
    "redirect-checker",
    "html-validation",
    "ai-seo",
    "geo",
    "hreflang",
    "eeat",
    "technical-seo"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seo-skills/seo-audit-skill.git"
  },
  "bugs": {
    "url": "https://github.com/seo-skills/seo-audit-skill/issues"
  },
  "homepage": "https://github.com/seo-skills/seo-audit-skill#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "log-update": "^6.1.0",
    "ora": "^8.1.1",
    "playwright": "^1.49.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "electron": "^40.4.0",
    "electron-builder": "^26.7.0",
    "electron-vite": "^5.0.0",
    "shadcn": "^3.7.0",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
